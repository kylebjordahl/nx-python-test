{
  "version": 2,
  "projects": {
    "flasktest": {
      "projectType": "application",
      "root": "apps/flasktest",
      "sourceRoot": "apps/flasktest/src",
      "targets": {
        "build": {
          "executor": "@nx-python/nx-python:build",
          "options": {
            "outputPath": "dist/apps/flasktest",
            "main": "apps/flasktest/src/hello.py",
            "templateType": "flask"
          }
        },
        "serve": {
          "executor": "@nx-python/nx-python:serve",
          "options": {
            "main": "apps/flasktest/src/hello.py",
            "templateType": "flask"
          }
        },
        "test": {
          "executor": "@nx-python/nx-python:test",
          "options": {
            "main": "apps/flasktest/src/test_hello.py",
            "templateType": "flask"
          }
        },
        "lint": {
          "executor": "@nx-python/nx-python:lint",
          "options": {
            "main": "apps/flasktest/src/hello.py",
            "templateType": "flask"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  }
}
